\begin_layout Section*
3.2 Linjära system och sinusar
\end_layout

\begin_layout Standard
\begin_inset listings
inline false
status open

\begin_layout Plain Layout

%Num = (s+0.1)(s+10) blir s^2+10.1s+1 
\end_layout

\begin_layout Plain Layout

num=[1 10.1 1];
\end_layout

\begin_layout Plain Layout

%Den = (s+1)(s^2+s+9) blir s^3+2s^2+10s+9 
\end_layout

\begin_layout Plain Layout

den=[1 2 10 9];
\end_layout

\begin_layout Plain Layout

sys=tf(num,den);
\end_layout

\begin_layout Plain Layout

bode(sys);
\end_layout

\begin_layout Plain Layout

pzmap(sys);
\end_layout

\begin_layout Plain Layout

t=0:0.01:81.92;
\end_layout

\begin_layout Plain Layout

x1=sin(1*t);
\end_layout

\begin_layout Plain Layout

x2=sin(3*t);
\end_layout

\begin_layout Plain Layout

x3=sin(5*t);
\end_layout

\begin_layout Plain Layout

y1=lsim(sys,x1,t);
\end_layout

\begin_layout Plain Layout

y2=lsim(sys,x2,t);
\end_layout

\begin_layout Plain Layout

y3=lsim(sys,x3,t);
\end_layout

\begin_layout Plain Layout

%verifiera ekvationen y(t) = g(ω)sin(ωt + φ(ω)) beräkna g(ω) och φ(ω) för
\end_layout

\begin_layout Plain Layout

% ω=3
\end_layout

\begin_layout Plain Layout

gw = (abs(evalfr(sys,3i)));
\end_layout

\begin_layout Plain Layout

fi = (angle(evalfr(sys,3i)));
\end_layout

\begin_layout Plain Layout

y = gw*sin(3*t + fi);
\end_layout

\begin_layout Plain Layout

% Plotta ut y2 och y
\end_layout

\begin_layout Plain Layout

plot(y2);
\end_layout

\begin_layout Plain Layout

hold on;
\end_layout

\begin_layout Plain Layout

plot(y);
\end_layout

\end_inset


\end_layout

