\begin_layout Section*
3.1 Fourierseriens uppbyggnad
\end_layout

\begin_layout Standard
Fourierkoefficienten C
\begin_inset script subscript

\begin_layout Plain Layout
k
\end_layout

\end_inset

för interfallet -30 till 30, med matlab.
\end_layout

\begin_layout Standard
\begin_inset listings
inline false
status open

\begin_layout Plain Layout

T=1;
\end_layout

\begin_layout Plain Layout

w=2*pi/T;
\end_layout

\begin_layout Plain Layout

M=200;
\end_layout

\begin_layout Plain Layout

t=T*(0:M-1)/M;
\end_layout

\begin_layout Plain Layout

y=zeros(1,200);
\end_layout

\begin_layout Plain Layout

ind=30; %intervall
\end_layout

\begin_layout Plain Layout

C= 1:(ind*2 + 1);
\end_layout

\begin_layout Plain Layout

for a = 1:(ind*2 + 1)
\end_layout

\begin_layout Plain Layout

	k = a - ind - 1;
\end_layout

\begin_layout Plain Layout

	syms p;
\end_layout

\begin_layout Plain Layout

	% Addera båda integralerna
\end_layout

\begin_layout Plain Layout

	C(a) = 1/T *(int(exp(-1i*p*w*k) , 0 , T/2 ) ...
 
\end_layout

\begin_layout Plain Layout

			+ int ( -exp(-1i*p*w*k) , T/2 , T));
\end_layout

\begin_layout Plain Layout

end
\end_layout

\begin_layout Plain Layout

for k = -ind:ind
\end_layout

\begin_layout Plain Layout

	a = k+ind + 1;
\end_layout

\begin_layout Plain Layout

	y = y + C(a)*exp(1i*w*t*k);
\end_layout

\begin_layout Plain Layout

end
\end_layout

\begin_layout Plain Layout

plot(t,y);
\end_layout

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Float figure
wide false
sideways false
status open

\begin_layout Plain Layout

\end_layout

